URL
re.findall(r'(?i)\b((?:https?://|www\d{0,3}[.]|[a-z0-9.\-]+[.][a-z]{2,4}/)(?:[^\s()<>]+|\(([^\s()<>]+|(\([^\s()<>]+\)))*\))+(?:\(([^\s()<>]+|(\([^\s()<>]+\)))*\)|[^\s`!()\[\]{};:'\".,<>?«»“”‘’]))',file)

IPV4
re.findall(r'^(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$',file)

IPV6
re.findall(r'^(?:[0-9a-fA-F]{1,4}:){7}[0-9a-fA-F]{1,4}$',file)

md5
re.findall(r'([a-fA-F\d]{32})',file)

sha256
re.findall(r'([a-fA-F\d]{64})',file)
